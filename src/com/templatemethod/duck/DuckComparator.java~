package com.templatemethod.duck;

import java.util.*;

public class DuckComparator implements Comparator{

  public int Compare(Object o1,Object o2) {
    
    Duck d1=(Duck)o1;
    Duck d2=(Duck)o2;
 
  //Duck otherDuck = (Duck)object;
  
  if (d1.weight < d2.weight) {
   return -1;
  } else if (d1.weight == d2.weight) {
   return 0;
  } else { //duck1.Duck() > duck2.Duck()
   return 1;
  }
 }

}